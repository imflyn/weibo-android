package net.weibo.app.bean;

/**
 * 微博实体类 text : 微博内容, origtext : 原始内容, count : 微博被转次数, mcount : 点评次数, from : 来源,
 * fromurl : 来源url, id : 微博唯一id, image : 图片url列表, video : { picurl : 缩略图, player
 * : 播放器地址, realurl : 视频原地址, shorturl : 视频的短url, title : 视频标题 }, music : {
 * author : 演唱者, url : 音频地址, title : 音频名字，歌名 }, name : 发表人帐户名, openid :
 * 用户唯一id，与name相对应, nick : 发表人昵称, self : 是否自已发的的微博，0-不是，1-是, timestamp : 发表时间,
 * type : 微博类型，1-原创发表，2-转载，3-私信，4-回复，5-空回，6-提及，7-评论, head : 发表者头像url, location :
 * 发表者所在地, country_code : 国家码（其他时间线一样）, province_code : 省份码（其他时间线一样）, city_code
 * : 城市码（其他时间线一样）, isvip : 是否微博认证用户，0-不是，1-是, geo : 发表者地理信息, status :
 * 微博状态，0-正常，1-系统删除，2-审核中，3-用户删除，4-根删除, emotionurl : 心情图片url, emotiontype :
 * 心情类型, source : 当type=2时，source即为源tweet
 * 
 * @author V
 * 
 */
public class News extends Message
{

    private int    count  = 0;
    private int    mcount = 0;
    private String from;
    private String fromurl;
    private String id;

    private String geo;
    private int    self;
    private int    type;
    private String head;
    private String location;
    private int    isvip;
    private int    status;
    private String emotionurl;
    private long   timestamp;
    private int    emotiontype;

    private News   source;

    // private PictureList pic;

    // public final PictureList getPic()
    // {
    // return pic;
    // }
    //
    // public final void setPic(PictureList pic)
    // {
    // this.pic = pic;
    // }

    public final int getCount()
    {
        return count;
    }

    public final void setCount(int count)
    {
        this.count = count;
    }

    public final int getMcount()
    {
        return mcount;
    }

    public final void setMcount(int mcount)
    {
        this.mcount = mcount;
    }

    public final String getFrom()
    {
        return from;
    }

    public final void setFrom(String from)
    {
        this.from = from;
    }

    public final String getFromurl()
    {
        return fromurl;
    }

    public final void setFromurl(String fromurl)
    {
        this.fromurl = fromurl;
    }

    public final String getId()
    {
        return id;
    }

    public final void setId(String id)
    {
        this.id = id;
    }

    public final String getGeo()
    {
        return geo;
    }

    public final void setGeo(String geo)
    {
        this.geo = geo;
    }

    public final int getSelf()
    {
        return self;
    }

    public final void setSelf(int self)
    {
        this.self = self;
    }

    public final int getType()
    {
        return type;
    }

    public final void setType(int type)
    {
        this.type = type;
    }

    public final String getHead()
    {
        return head;
    }

    public final void setHead(String head)
    {
        this.head = head;
    }

    public final String getLocation()
    {
        return location;
    }

    public final void setLocation(String location)
    {
        this.location = location;
    }

    public final int getIsvip()
    {
        return isvip;
    }

    public final void setIsvip(int isvip)
    {
        this.isvip = isvip;
    }

    public final int getStatus()
    {
        return status;
    }

    public final void setStatus(int status)
    {
        this.status = status;
    }

    public final String getEmotionurl()
    {
        return emotionurl;
    }

    public final void setEmotionurl(String emotionurl)
    {
        this.emotionurl = emotionurl;
    }

    public final int getEmotiontype()
    {
        return emotiontype;
    }

    public final void setEmotiontype(int emotiontype)
    {
        this.emotiontype = emotiontype;
    }

    public final long getTimestamp()
    {
        return timestamp;
    }

    public final void setTimestamp(long timestamp)
    {
        this.timestamp = timestamp;
    }

    public final News getSource()
    {
        return source;
    }

    public final void setSource(News source)
    {
        this.source = source;
    }

}
