package net.weibo.app.bean;

/**
 * 通知信息实体类
 */
public class Notice
{
    public final static String UTF8         = "UTF-8";
    public final static String NODE_ROOT    = "QQWeibo";

    public final static int    TYPE_ATME    = 1;
    public final static int    TYPE_MESSAGE = 2;
    public final static int    TYPE_COMMENT = 3;
    public final static int    TYPE_NEWFAN  = 4;
    public final static int    TYPE_INFORM  = 5;

    private int                atmeCount;
    private int                msgCount;
    private int                reviewCount;
    private int                newFansCount;
    private int                inFormCount;

    public final int getInFormCount()
    {
        return inFormCount;
    }

    public final void setInFormCount(int inFormCount)
    {
        this.inFormCount = inFormCount;
    }

    public final int getAtmeCount()
    {
        return atmeCount;
    }

    public final void setAtmeCount(int atmeCount)
    {
        this.atmeCount = atmeCount;
    }

    public final int getMsgCount()
    {
        return msgCount;
    }

    public final void setMsgCount(int msgCount)
    {
        this.msgCount = msgCount;
    }

    public final int getReviewCount()
    {
        return reviewCount;
    }

    public final void setReviewCount(int reviewCount)
    {
        this.reviewCount = reviewCount;
    }

    public final int getNewFansCount()
    {
        return newFansCount;
    }

    public final void setNewFansCount(int newFansCount)
    {
        this.newFansCount = newFansCount;
    }

}
